{
  "name": "my-server-fastify",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx -r tsconfig-paths/register index.ts",
    "dex": "bun --hot index.ts",
    "start": "node dist/index.js",
    "devs": "npm run prisma && npm run dev",
    "prisma": "npx prisma generate && npx prisma migrate dev",
    "clean": "rimraf dist",
    "copy-views": " copyfiles -u 1 src/**/*.ejs dist/src",
    "build": "npm run build:server && npm run build:client",
    "build:server": "npm run clean && tsc && tsc-alias -p tsconfig.json && npm run copy-views && node scripts/add-js-ext.js",
    "build:client": "rimraf dist/src/client && vite build",
    "production": "npm run build && npm run start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "private": true,
  "description": "",
  "engines": {
    "node": ">= 20"
  },
